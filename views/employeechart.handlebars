<ul class="welcome">
  {{#each forthisemployee}}
    &nbsp;&nbsp;
    

  {{/each}}
</ul>

<div class="row">
  <div class="col s12"></div>
  <div class="col s6">

    <ul class="list">
      {{#each employeeTasks}}
        <li>{{name}}{{tasks.[0].[2]}}
         

        </li>
             {{name.task}}</li>
      {{/each}}
    </ul>
    <div class="input-field col s12">
      <select>
        <optgroup label="team 1">

          {{#each forthisemployee}}

            <li>{{this}}</li>
            <option value="1">{{this}}</option>
          {{/each}}

          <option value="2">Option 2</option>
        </optgroup>
        <optgroup label="team 2">
          <option value="3">Option 3</option>
          <option value="4">Option 4</option>
        </optgroup>
      </select>
      <label>Optgroups</label>
    </div>
  </div>
  <div class="col s6"><canvas id="myChart"></canvas></div>
  <div>
    <ul class="collect">
      <li>
        {{#each employeeTasks as |worker|}}

          <button
            class="waves-effect waves-light btn"
            data-id="{{worker.id}}"
          >{{worker.name}}</button>
        {{/each}}
      </li>

    </ul>
  </div>
</div>
<script src="./js/chart-employees.js">

</script>