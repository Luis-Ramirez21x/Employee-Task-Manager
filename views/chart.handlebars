<h5>Welcome{{userforchart.name}} + {{employeeTasks.[0].id}}+ This is for Managers!</h5>

<div class="row">
  <div class="col s12"></div>
  <div class="col s6">
    {{#each employeeTasks as |worker|}}

           <ul class="collection">
            
            <li class="collection-item avatar">
              <i class="material-icons circle">folder</i>
              <span class="titleWorker"> {{worker.name}}</span>
                  {{#each tasks as |task|}}
              </p>
              <p> {{title}}
                <br />
              </p>
                {{/each}}
              <a href="#!" class="secondary-content"><i
                  class="material-icons"
                >grade</i></a>
            </li>
        
      </ul>

    {{/each}}
    
  </div>
  <div class="col s6"><canvas id="myChart"></canvas></div>
  <div>
    <ul class="collect">
        <li>
        {{#each employeeTasks as |worker|}}
      
        <button class="waves-effect waves-light btn" data-id="{{worker.id}}">{{worker.name}}</button>
    {{/each}}
         </li>
    
      </ul>
  </div>
</div>
